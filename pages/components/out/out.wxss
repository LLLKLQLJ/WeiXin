.one,.two{
  margin:15px 20px 0 20px;
  font-size: .55rem ;
}
.two text{
  display:block;
}
.numb{
border:1px solid  #ddd;
border-radius: 2em;
width:100%;
height:30px;
} 

.numb1{
border:1px solid  #ddd;
border-radius: 2em;
width:40%;
height:30px;
display:block;
float:left;
margin-right:2px;

} 
.numb2{
border:1px solid  green;
border-radius: 2em;
width:55%;
height:30px;
display:block;
float:right;
} 
.three{
margin:50px 20px 0 20px;
  font-size: .55rem ;
  clear:both;
}
.three text{
  display:block;
}
.btn{
  width: 87%;
  height:30px;
  border:1px solid green;
  margin:30px 20px 0 20px;
  background:green;
  color:white;
  font-size:.55rem ;
}
.p{
  display:block;
   margin:30px 20px 30px 20px;
   font-size: .60rem ;
   height:30px;
   line-height:30px;
}